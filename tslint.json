{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-namespace": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
